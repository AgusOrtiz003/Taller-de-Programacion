  A10:AreaP(13,16,14,16)
  A11:AreaP(13,17,13,18)
  A12:AreaP(10,18,12,18)
  A13:AreaP(10,19,10,20)
  A14:AreaP(6,21,10,21)
  A15:AreaP(6,22,6,25)
  A16:AreaP(1,25,5,25)
  A17:AreaP(1,26,1,30)
  B1:AreaP(31,1,31,5)
  B2:AreaP(26,6,31,6)
  B3:AreaP(26,7,26,9)
  B4:AreaP(22,10,26,10)
  B5:AreaP(22,11,22,13)
  B6:AreaP(19,13,22,13)
  B7:AreaP(19,14,19,15)
  B8:AreaP(17,16,19,16)
  B9:AreaP(19,17,19,18)
  B10:AreaP(20,18,22,18)
  B11:AreaP(22,19,22,21)
  B12:AreaP(22,22,25,22)
  B13:AreaP(26,23,26,26)
  B14:AreaP(27,26,31,26)
  B15:AreaP(31,27,31,31)
robots
  robot robotF
  variables
    indice:numero
    f:numero
  comenzar
    f:=0
    repetir 2
      indice:=5
      mientras(indice>0)
        escalonF(indice,f)
      izquierda
  fin
variables
  R_info: robot1
comenzar
  AsignarArea(R_info, ciudad)
  Iniciar(R_info, 1,1)
fin
