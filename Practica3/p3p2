programa p3p2
procesos
  proceso izquierda
  comenzar
    repetir 3
      derecha
  fin
  proceso escalonF(ES indice:numero;ES f:numero)
  comenzar
    repetir indice
      juntarFlores(f)
      mover
    derecha
    repetir indice
      juntarFlores(f)
      mover
      si((PosAv==15)&&(PosCa==16))
        BloquearEsquina(16,16)
    izquierda
    indice:=indice-1
  fin
  proceso escalonP(ES indice:numero;ES p:numero)
  comenzar
    repetir indice
      juntarPapeles(p)
      mover
    derecha
    repetir indice
      juntarPapeles(p)
      mover
    izquierda
    indice:=indice-1
  fin
areas
  areaJ:AreaP(15,1,15,1)
  areaCentral:AreaPC(16,16,16,16)
  A1:AreaP(1,1,1,5)
  A2:AreaP(1,6,5,6)
  A3:AreaP(6,6,6,9)
  A4:AreaP(6,10,10,9)
